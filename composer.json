{
    "name": "defectivecode/laravel-sqs-extended",
    "type": "laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DefectiveCode\\LaravelSqsExtended\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DefectiveCode\\LaravelSqsExtended\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Corey McCormick",
            "email": "corey@defectivecode.com"
        }
    ],
    "suggest": {
        "laravel/vapor-core": "Required for SQS disk-based storage when using Laravel Vapor."
    },
    "require": {
        "php": "^8.4",
        "aws/aws-sdk-php": "^3.368.0.0",
        "illuminate/container": "^10.0 || ^11.0 || ^12.0",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "illuminate/filesystem": "^10.0 || ^11.0 || ^12.0",
        "illuminate/queue": "^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DefectiveCode\\LaravelSqsExtended\\SqsDiskServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    }
}
